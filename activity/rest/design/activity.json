{
  "name": "tibco-rest",
  "version": "0.1.0",
  "description": "Query a pet's infromation",
  "title": "TIBCO Pet Query",
  "inputs": [
    {
      "name": "petId",
      "title": "Pet ID",
      "value": "222",
      "required": true,
      "validation": "^\\d*$",
      "validationMessage": "Pet ID Should be a valid number"
    },
    {
      "name": "uri",
      "title": "URI",
      "value": "http://petstore.swagger.io/v2/pet/{petId}",
      "required": true
    },
    {
      "name": "method",
      "title": "Method",
      "value": "GET",
      "required": true
    }
  ],
  "outputs": [
    {
      "name": "result",
      "type": "object"
    }
  ]
}
